{
  "name": "eslint-plugin-code-complete",
  "version": "1.0.0",
  "description": "ESLint rules to help write complete, maintainable code with better readability",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./rules": "./dist/rules/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsc --noEmitOnError false",
    "build:force": "tsc || true",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "clean": "find . -name '.DS_Store' -type f -delete"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Arye Lukashevski <arye.lu@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@8.6.0"
}